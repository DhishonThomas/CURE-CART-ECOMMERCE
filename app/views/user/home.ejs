<%- include('../partialsUser/header'); -%>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<div class="container">
    <hr class="mb-4">

    <!-- <h2 class="title text-center mb-3">Large Banner</h2>End .title mb-2 -->

    <div class="row">
        <div class="col-15">
            <div class="banner banner-big">
                <a href="/shop">
                    <img src="userCss/images/banners/banner-fullwidth.jpg" alt="Banner">
                </a>

                <div class="banner-content">
                    <h2 class="banner-subtitle" style="color: black;">New Arrivals</h2>
                    <!-- End .banner-subtitle
                    <h3 class="banner-title text-white">Morbi interdum <br>mollis sapien.</h3>End .banner-title
                    <p class="d-none d-lg-block">Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Donec odio. Quisque volutpat mattis eros. Nullam malesuada erat ut turpis. Suspendisse urna nibh, viverra non, posuere a, pede.</p> -->

                    <a href="/shop" class="btn btn-success btn-rounded" style="border-radius: 5px;color: aquamarine;"><span>Click Here</span><i class="icon-long-arrow-right"></i></a>
                </div><!-- End .banner-content -->
            </div><!-- End .banner -->
        </div><!-- End .col-12 -->
    </div><!-- End .row -->
</div>

<div class="container new-arrivals">
  <div class="heading heading-flex mb-3">
    <div class="heading-left">
      <h2 class="title">Aurevedic</h2>
      <!-- End .title -->
    </div>
    <!-- End .heading-left -->

    <div class="heading-right">
      <ul
        class="nav nav-pills nav-border-anim justify-content-center"
        role="tablist"
      >
    
      </ul>
    </div>
    <!-- End .heading-right -->
  </div>
  

  <div class="tab-content tab-content-carousel just-action-icons-sm">
    <div
      class="tab-pane p-0 fade show active"
      id="new-all-tab"
      role="tabpanel"
      aria-labelledby="new-all-link"
    >
      <div
        class="owl-carousel owl-full carousel-equal-height carousel-with-shadow"
        data-toggle="owl"
        data-owl-options='{
                                "nav": true, 
                                "dots": true,
                                "margin": 20,
                                "loop": false,
                                "responsive": {
                                    "0": {
                                        "items":2
                                    },
                                    "480": {
                                        "items":2
                                    },
                                    "768": {
                                        "items":3
                                    },
                                    "992": {
                                        "items":4
                                    },
                                    "1200": {
                                        "items":5
                                    }
                                }
                            }'
      >
      <% aurevedic.forEach(product => { %>
       
      
        <div class="product product-2 ">
          <figure class="product-media">
            <!-- <span class="product-label label-circle label-top">Top</span> -->
            <a href="/product/<%= product._id %>">
              <img
                src="<%= product.images[0] %>"
                alt="Product image"
                class="product-image"
              />
            </a>

            <div class="product-action-vertical">
              <a
                href="#"
                class="btn-product-icon btn-wishlist"
                title="Add to wishlist"
              ></a>
            </div>
            <!-- End .product-action -->

            <div class="product-action">
              <% if (user) { %>
                <a class="btn-product btn-cart" id="Wishlist" title="Add to cart">
                  <span>add to cart</span>
                </a>
              <% } else { %>
                <a class="btn-product btn-cart noWishlist" title="Add to cart">
                  <span>add to cart</span>
                </a>
              <% } %>      
              
              <% if (user) { %>
                <a
                  href="/product/<%= product._id %>"
                  class="btn-product btn-quickview"
                  title="Quick view"
                >
                  <span>quick view</span>
                </a>
              <% } else { %>
                <a
                  href="/product/<%= product._id %>"
                  class="btn-product btn-quickview"
                  title="Quick view"
                >
                  <span>quick view</span>
                </a>
              <% } %>         
            </div>
            <!-- End .product-action -->
          </figure>
          <!-- End .product-media -->

          <div class="product-body">
            <div class="product-cat">
              <a href="#"></a>
            </div>
            <!-- End .product-cat -->
            <h3 class="product-title">
              <a href="product.html">
                <% let productName =product.productName.substring(0, 40).trim()+"..." %>
                <%= productName%></a>
            </h3>
            <!-- End .product-title -->
            <div>
              <div style="color: blue; font-size: larger; font-weight: bold;">₹<%= product.price %></div> 

             </div>
            <!-- End .rating-container -->
          </div>
          <!-- End .product-body -->
        </div>
        <!-- End .product -->
<% }) %>

    <!-- .End .tab-pane -->

</div>
<!-- End .container -->
</div>
</div></div>


