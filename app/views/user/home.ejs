<script>
   const noWishlistButtons = document.querySelectorAll(".noWishlist");

noWishlistButtons.forEach(button => {
  button.addEventListener('click', function () {
    Swal.fire({
      icon: 'info',
      title: 'Login Required',
      text: 'Please Login To Add Items To Wishlist',
    });
  });
});

</script>

